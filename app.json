{
  "expo": {
    "name": "meme-generator",
    "slug": "meme-generator",
    "version": "1.0.0",
    "owner": "ajaysharma12799",
    "description": "A Simple Meme Generator",
    "orientation": "portrait",
    "icon": "./assets/meme-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Splash-Scree.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/meme-logo.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}